<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" >
    <style name="Widget.Button.Toggle" parent="android:Widget">
        <item name="android:background">@drawable/btn_toggle_bg</item>
        <item name="android:textOn">0</item>
        <item name="android:textOff">Get Redeem Code</item>
        <item name="android:disabledAlpha">0</item>
        
    </style>
    
    <style name="header_base">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="dark_header_base">
        <item name="android:textColor">@color/black</item>
    </style>    
    <style name="header" parent="header_base">
        <item name="android:textSize">22dip</item>
    </style>
    <style name="bold_header" parent="header_base">
        <item name="android:textSize">18dip</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="dark_bold_header" parent="dark_header_base">
        <item name="android:textSize">18dip</item>
        <item name="android:textStyle">bold</item>
    </style>    
</resources>